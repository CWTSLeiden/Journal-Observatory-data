{
    "@base": "https://doaj.org/",
    "job": "https://job.org/",
    "schema": "https://schema.org/",
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "doaj": "https://doaj.org/",
    "cc": "https://creativecommons.org/licenses/",
    "id": "@id",
    "created_date": {
        "@id": "schema:dateCreated",
        "@type": "schema:DateTime"
    },
    "last_updated": {
        "@id": "schema:dateModified",
        "@type": "schema:DateTime"
    },
    "bibjson": {
        "@id": "@nest"
    },
    "eissn": {
        "@id": "schema:eissn"
    },
    "pissn": {
        "@id": "schema:pissn"
    },
    "issn": {
        "@id": "schema:issn"
    },
    "title": {
        "@id": "schema:name"
    },
    "alternative_title": {
        "@id": "schema:alternateName"
    },
    "price": {
        "@id": "schema:price",
        "@type": "xsd:integer"
    },
    "currency": {
        "@id": "schema:priceCurrency",
        "@type": "xsd:string"
    },
    "country": {
        "@id": "schema:addressCountry",
        "@type": "xsd:string"
    },
    "copyright": {
        "@id": "doaj:AuthorCopyright",
        "@context": {
            "author_retains": {
                "@id": "doaj:authorRetainsCopyright",
                "@type": "xsd:boolean"
            },
            "url": {
                "@id": "doaj:copyrightPolicy"
            }
        }
    },
    "keywords": {
        "@id": "doaj:keyword"
    },
    "publisher": {
        "@id": "doaj:Publisher",
        "@context": {
            "name": {
                "@id": "schema:name"
            }
        }
    },
    "apc": {
        "@id": "doaj:hasArticlePublishingCharges",
        "@context": {
            "has_apc": {
                "@id": "doaj:ArticlePublishingChargesApply",
                "@type": "xsd:boolean"
            },
            "max": {
                "@id": "schema:PriceSpecification",
                "@context": {
                    "price": {
                        "@id": "schema:MaxPrice",
                        "@type": "xsd:integer"
                    }
                }
            }
        }
    }
}
