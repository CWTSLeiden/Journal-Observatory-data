{
    "@base": "https://doaj.org/",
    "ppo": "http://purl.org/cwts/ppo/",
    "schema": "https://schema.org/",
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "doaj": "https://doaj.org/",
    "fabio": "http://purl.org/spar/fabio/",
    "dcterms": "http://purl.org/dc/terms/",
    "id": "@id",
    "created_date": {
        "@id": "schema:dateCreated",
        "@type": "xsd:dateTime"
    },
    "last_updated": {
        "@id": "schema:dateModified",
        "@type": "xsd:dateTime"
    },
    "name": {
        "@id": "schema:name"
    },
    "url": {
        "@id": "schema:url"
    },
    "bibjson": {
        "@id": "@nest"
    },
    "eissn": {
        "@id": "prism:eIssn"
    },
    "pissn": {
        "@id": "prism:issn"
    },
    "title": {
        "@id": "schema:name"
    },
    "alternative_title": {
        "@id": "schema:alternateName"
    },
    "price": {
        "@id": "schema:price",
        "@type": "xsd:integer"
    },
    "currency": {
        "@id": "schema:priceCurrency",
        "@type": "xsd:string"
    },
    "country": {
        "@id": "schema:addressCountry",
        "@type": "xsd:string"
    },
    "copyright": {
        "@id": "doaj:Copyright",
        "@context": {
            "author_retains": {
                "@id": "doaj:authorRetainsCopyright",
                "@type": "xsd:boolean"
            }
        }
    },
    "keywords": {
        "@id": "doaj:keyword"
    },
    "publisher": {
        "@id": "doaj:Publisher"
    },
    "apc": {
        "@id": "ppo:hasArticleProcessingCharge",
        "@context": {
            "has_apc": {
                "@id": "doaj:ArticleProcessingChargeApplies",
                "@type": "xsd:boolean"
            },
            "max": {
                "@id": "schema:PriceSpecification",
                "@context": {
                    "price": {
                        "@id": "schema:MaxPrice",
                        "@type": "xsd:integer"
                    }
                }
            }
        }
    },
    "license": {
        "@id": "dcterms:license",
        "@context": {
            "type": "doaj:licenseType"
        }
    }
}
