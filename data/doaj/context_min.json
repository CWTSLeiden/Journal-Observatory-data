{
    "job": "https://job.org/",
    "schema": "https://schema.org/",
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "doaj": "https://doaj.org/schema#",
    "cc": "https://creativecommons.org/licenses/",
    "id": {
        "@id": "@id"
    },
    "created_date": {
        "@id": "schema:dateCreated",
        "@type": "schema:DateTime"
    },
    "last_updated": {
        "@id": "schema:dateModified",
        "@type": "schema:DateTime"
    },
    "bibjson": {
        "@id": "@nest"
    },
    "eissn": {
        "@id": "schema:eissn"
    },
    "pissn": {
        "@id": "schema:pissn"
    },
    "issn": {
        "@id": "schema:issn"
    },
    "price": {
        "@id": "schema:price",
        "@type": "xsd:integer"
    },
    "currency": {
        "@id": "schema:priceCurrency",
        "@type": "schema:text"
    },
    "apc": {
        "@id": "job:ArticlePublishingCharges",
        "@context": {
            "has_apc": {
                "@id": "job:hasArticlePublishingCharges",
                "@type": "xsd:boolean"
            },
            "max": {
                "@id": "schema:PriceSpecification",
                "@context": {
                    "price": {
                        "@id": "schema:MaxPrice",
                        "@type": "xsd:integer"
                    }
                }
            }
        }
    }
}
