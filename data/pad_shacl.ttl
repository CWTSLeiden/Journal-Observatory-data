sh:PADShape a sh:NodeShape ;
    sh:targetClass ppo:PAD ;
    sh:not [
        sh:class ppo:Provenance ;
        sh:class ppo:DocInfo
    ] ;
    sh:property [ a sh:PropertyShape ;
        sh:path ppo:hasAssertion ;
        sh:class ppo:Assertion ;
        sh:minCount 1 ;
        sh:maxCount 1 
    ] ;
    sh:property [ a sh:PropertyShape ;
        sh:path ppo:hasProvenance ;
        sh:class ppo:Provenance ;
        sh:minCount 1 ;
        sh:maxCount 1 
    ] .


